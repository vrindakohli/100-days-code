#include<bits/stdc++.h>
using namespace std;

void ArrayInter(int a[], int n, int b[], int m)
{
    int i,j;
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            if(a[i]==b[j]){
                cout<<a[i]<<" ";
            }
        }
    }
}

int main()
{
    int m,n;
    cout<<"size of array1";
    cin>>n;
    cout<<"size of array2";
    cin>>m;
    int a[n], b[m];
    cout<<"enter array1";
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    cout<<"enter array2";
    for(int i=0;i<m;i++){
        cin>>b[i];
    }
    ArrayInter(a,n,b,m);
    return 0;
}

